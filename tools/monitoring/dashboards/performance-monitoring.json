{
  "dashboard": {
    "id": "performance-monitoring",
    "title": "NeonPro Healthcare Platform - Performance Monitoring",
    "description": "Real-time performance monitoring for healthcare operations",
    "tags": ["healthcare", "performance", "production"],
    "timezone": "America/Sao_Paulo",
    "panels": [
      {
        "id": "overview-stats",
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"neonpro-web\"}",
            "legendFormat": "Web Server"
          },
          {
            "expr": "up{job=\"neonpro-api\"}",
            "legendFormat": "API Server"
          },
          {
            "expr": "neonpro_database_healthy",
            "legendFormat": "Database"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN"
                  },
                  "1": {
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": "response-times",
        "title": "API Response Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, neonpro_api_request_duration_seconds_bucket)",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.90, neonpro_api_request_duration_seconds_bucket)",
            "legendFormat": "90th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, neonpro_api_request_duration_seconds_bucket)",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, neonpro_api_request_duration_seconds_bucket)",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": "error-rates",
        "title": "Error Rates by Service",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(neonpro_api_errors_total{service=\"patients\"}[5m])",
            "legendFormat": "Patients Service"
          },
          {
            "expr": "rate(neonpro_api_errors_total{service=\"appointments\"}[5m])",
            "legendFormat": "Appointments Service"
          },
          {
            "expr": "rate(neonpro_api_errors_total{service=\"medical-records\"}[5m])",
            "legendFormat": "Medical Records Service"
          },
          {
            "expr": "rate(neonpro_api_errors_total{service=\"billing\"}[5m])",
            "legendFormat": "Billing Service"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": "throughput",
        "title": "Request Throughput",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(neonpro_api_requests_total[5m])",
            "legendFormat": "Total Requests"
          },
          {
            "expr": "rate(neonpro_api_requests_total{method=\"GET\"}[5m])",
            "legendFormat": "GET Requests"
          },
          {
            "expr": "rate(neonpro_api_requests_total{method=\"POST\"}[5m])",
            "legendFormat": "POST Requests"
          },
          {
            "expr": "rate(neonpro_api_requests_total{method=\"PUT\"}[5m])",
            "legendFormat": "PUT Requests"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 18
        }
      },
      {
        "id": "database-performance",
        "title": "Database Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "neonpro_database_query_duration_seconds{quantile=\"0.95\"}",
            "legendFormat": "95th percentile query time"
          },
          {
            "expr": "neonpro_database_connections_active",
            "legendFormat": "Active connections"
          },
          {
            "expr": "neonpro_database_connections_idle",
            "legendFormat": "Idle connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 18
        }
      },
      {
        "id": "memory-usage",
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "neonpro_memory_usage_bytes{job=\"neonpro-web\"} / 1024 / 1024 / 1024",
            "legendFormat": "Web Server (GB)"
          },
          {
            "expr": "neonpro_memory_usage_bytes{job=\"neonpro-api\"} / 1024 / 1024 / 1024",
            "legendFormat": "API Server (GB)"
          },
          {
            "expr": "neonpro_memory_usage_bytes{job=\"neonpro-database\"} / 1024 / 1024 / 1024",
            "legendFormat": "Database (GB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": "cpu-usage",
        "title": "CPU Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "neonpro_cpu_usage_percent{job=\"neonpro-web\"}",
            "legendFormat": "Web Server"
          },
          {
            "expr": "neonpro_cpu_usage_percent{job=\"neonpro-api\"}",
            "legendFormat": "API Server"
          },
          {
            "expr": "neonpro_cpu_usage_percent{job=\"neonpro-database\"}",
            "legendFormat": "Database"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 28
        }
      },
      {
        "id": "healthcare-specific",
        "title": "Healthcare-Specific Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "neonpro_patients_active_total",
            "legendFormat": "Active Patients"
          },
          {
            "expr": "neonpro_appointments_today_total",
            "legendFormat": "Today's Appointments"
          },
          {
            "expr": "neonpro_medical_records_access_today_total",
            "legendFormat": "Medical Records Accessed"
          },
          {
            "expr": "neonpro_prescriptions_filled_today_total",
            "legendFormat": "Prescriptions Filled"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 38
        }
      },
      {
        "id": "patient-data-access",
        "title": "Patient Data Access Patterns",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(neonpro_patient_data_access_total[5m])",
            "legendFormat": "Data Access Rate"
          },
          {
            "expr": "rate(neonpro_patient_data_modifications_total[5m])",
            "legendFormat": "Data Modifications"
          },
          {
            "expr": "rate(neonpro_patient_data_exports_total[5m])",
            "legendFormat": "Data Exports"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 46
        }
      },
      {
        "id": "appointment-metrics",
        "title": "Appointment System Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(neonpro_appointments_created_total[5m])",
            "legendFormat": "Appointments Created"
          },
          {
            "expr": "rate(neonpro_appointments_completed_total[5m])",
            "legendFormat": "Appointments Completed"
          },
          {
            "expr": "rate(neonpro_appointments_cancelled_total[5m])",
            "legendFormat": "Appointments Cancelled"
          },
          {
            "expr": "rate(neonpro_appointments_failed_total[5m])",
            "legendFormat": "Appointments Failed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 46
        }
      },
      {
        "id": "compliance-performance",
        "title": "Compliance System Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "neonpro_compliance_check_duration_seconds",
            "legendFormat": "Compliance Check Duration"
          },
          {
            "expr": "neonpro_audit_log_processing_duration_seconds",
            "legendFormat": "Audit Log Processing"
          },
          {
            "expr": "neonpro_encryption_operations_duration_seconds",
            "legendFormat": "Encryption Operations"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 56
        }
      },
      {
        "id": "cdn-performance",
        "title": "CDN Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "neonpro_cdn_cache_hit_ratio",
            "legendFormat": "Cache Hit Ratio"
          },
          {
            "expr": "neonpro_cdn_response_time_seconds",
            "legendFormat": "CDN Response Time"
          },
          {
            "expr": "rate(neonpro_cdn_requests_total[5m])",
            "legendFormat": "CDN Requests"
          }
        ],
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 66
        }
      },
      {
        "id": "mobile-performance",
        "title": "Mobile Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "neonpro_mobile_load_time_seconds",
            "legendFormat": "Mobile Load Time"
          },
          {
            "expr": "neonpro_mobile_interactive_time_seconds",
            "legendFormat": "Mobile Interactive Time"
          },
          {
            "expr": "neonpro_mobile_first_contentful_paint_seconds",
            "legendFormat": "Mobile FCP"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 66
        }
      },
      {
        "id": "network-performance",
        "title": "Network Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "neonpro_network_incoming_bytes_total / 1024 / 1024",
            "legendFormat": "Incoming (MB)"
          },
          {
            "expr": "neonpro_network_outgoing_bytes_total / 1024 / 1024",
            "legendFormat": "Outgoing (MB)"
          },
          {
            "expr": "neonpro_network_connections_active",
            "legendFormat": "Active Connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 76
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "environment",
          "query": "production,staging,development",
          "type": "custom"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": false,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    }
  }
}
