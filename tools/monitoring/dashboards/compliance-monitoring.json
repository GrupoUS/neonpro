          },
          {
            "expr": "neonpro_lgpd_data_portability_requests_total",
            "legendFormat": "Portability Requests"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
