{
  "dashboard": {
    "id": "compliance-monitoring",
    "title": "NeonPro Healthcare Platform - Compliance Monitoring",
    "description": "Real-time healthcare compliance monitoring dashboard",
    "tags": ["healthcare", "compliance", "lgpd", "anvisa", "cfm"],
    "timezone": "America/Sao_Paulo",
    "panels": [
      {
        "id": "compliance-overview",
        "title": "Compliance Status Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "neonpro_compliance_lgpd_status",
            "legendFormat": "LGPD"
          },
          {
            "expr": "neonpro_compliance_anvisa_status",
            "legendFormat": "ANVISA"
          },
          {
            "expr": "neonpro_compliance_cfm_status",
            "legendFormat": "CFM"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "NON-COMPLIANT"
                  },
                  "1": {
                    "text": "COMPLIANT"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": "lgpd-compliance",
        "title": "LGPD Compliance Metrics",
        "type": "timeseries",
