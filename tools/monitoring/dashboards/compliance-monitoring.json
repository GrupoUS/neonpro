    "templating": {
      "list": [
        {
          "name": "compliance_framework",
          "query": "lgpd,anvisa,cfm,hipaa,gdpr",
          "type": "custom"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
