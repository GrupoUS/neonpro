{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "tsconfig.json",
    "bun.lockb",
    "package.json"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "build:check": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [
        "coverage/**"
      ]
    },
    "test:e2e": {
      "dependsOn": ["build"]
    },
    "test:coverage": {
      "dependsOn": ["build"],
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "lint:edge": {
      "outputs": []
    },
    "type-check": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "validate": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "quality": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "db:migrate": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["build", "test"],
      "cache": false
    },
    "deploy:staging": {
      "dependsOn": ["build", "test"],
      "cache": false
    }
  }
}