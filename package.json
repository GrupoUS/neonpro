{
  "name": "neonpro-monorepo",
  "version": "1.0.0",
  "description": "NeonPro aesthetic clinic management platform - Turborepo monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "esbuild": ">=0.25.0",
    "hono": ">=4.9.7",
    "tmp": ">=0.2.4",
    "form-data": ">=2.5.4",
    "d3-color": ">=3.1.0",
    "ws": ">=8.17.1",
    "cross-spawn": ">=6.0.6",
    "tar-fs": ">=2.1.3",
    "got": ">=11.8.5",
    "tough-cookie": ">=4.1.3",
    "cookie": ">=0.7.0",
    "vite": "7.1.7",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --concurrency=15",
    "dev:web": "turbo dev --filter=@neonpro/web",
    "dev:api": "turbo dev --filter=@neonpro/api",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "oxlint . --import-plugin --react-plugin --jsx-a11y-plugin",
    "lint:fix": "oxlint . --import-plugin --react-plugin --jsx-a11y-plugin --fix",
    "lint:security": "eslint . --max-warnings 0",
    "lint:biome": "biome lint .",
    "lint:biome:fix": "biome lint --fix .",
    "type-check": "turbo type-check",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "format:check": "biome format .",
    "biome:check": "biome check .",
    "biome:fix": "biome check --fix .",
    "quality": "pnpm lint && pnpm lint:security && pnpm type-check",
    "quality:fix": "pnpm lint:fix && pnpm lint:security",
    "clean": "turbo clean",
    "prepare": "husky",
    "test:healthcare-compliance": "vitest run --config vitest.config.ts --reporter=verbose packages/healthcare-core/src/__tests__/",
    "test:security-compliance": "vitest run --config vitest.config.ts --reporter=verbose packages/security/src/__tests__/",
    "test:regulatory-compliance": "vitest run --config vitest.config.ts --reporter=verbose apps/api/src/__tests__/services/",
    "test:compliance-report": "vitest run --config vitest.config.ts --reporter=verbose --coverage.dir=coverage-compliance",
    "test:audit-compliance": "vitest run --config vitest.config.ts --reporter=verbose",
    "test:all-compliance": "pnpm test:healthcare-compliance && pnpm test:security-compliance && pnpm test:regulatory-compliance",
    "test:accessibility": "bash scripts/accessibility-check.sh",
    "quality:complete": "pnpm lint && pnpm lint:security && pnpm type-check && pnpm biome:check && pnpm test:accessibility"
  },
  "keywords": [
    "neonpro",
    "healthcare",
    "aesthetic-clinic",
    "turborepo",
    "monorepo",
    "vite",
    "react",
    "typescript"
  ],
  "author": "NeonPro Development Team",
  "license": "MIT",
  "dependencies": {
    "@hono/trpc-server": "0.4.0",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-progress": "1.1.7",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tabs": "1.1.13",
    "@trpc/server": "11.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "ai": "5.0.54",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dompurify": "3.2.7",
    "dotenv": "17.2.2",
    "ioredis": "5.8.0",
    "lodash-es": "4.17.21",
    "lodash.debounce": "4.0.8",
    "lovable-tagger": "1.1.10",
    "tailwind-merge": "3.3.1",
    "turbo": "2.5.8",
    "valibot": "1.1.0",
    "vite": "7.1.7",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
