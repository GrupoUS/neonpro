{
  "name": "neonpro",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:api": "turbo run dev --filter=api",
    "dev:web": "turbo run dev --filter=web", 
    "dev:full": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "npx ultracite format",
    "format:check": "npx @biomejs/biome check .",
    "lint:biome": "npx ultracite lint",
    "check:fix": "npx ultracite format",
    "ci-check": "npx @biomejs/biome ci .",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:unit": "cd tools/testing && pnpm vitest",
    "test:e2e": "cd tools/testing && pnpm test:playwright",
    "test:integration": "turbo run test:integration",
    "clean": "turbo run clean",
    "compliance:check": "turbo run compliance:check",
    "compliance:lgpd": "turbo run compliance:lgpd",
    "compliance:anvisa": "turbo run compliance:anvisa",
    "compliance:cfm": "turbo run compliance:cfm",
    "security:audit": "turbo run security:audit",
    "ai:build-models": "turbo run build:ai-models",
    "schedule:optimize": "turbo run schedule:optimize",
    "treatments:validate": "turbo run treatments:validate"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@turbo/gen": "^1.13.4",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.1",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "node-mocks-http": "^1.17.2",
    "sonner": "^2.0.7",
    "turbo": "^1.13.4",
    "typescript": "^5.0.0",
    "ultracite": "^5.2.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.2",
    "@tanstack/react-query-devtools": "^5.62.2",
    "@vitejs/plugin-react": "^5.0.0",
    "jspdf": "^3.0.1",
    "next": "^15.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^3.4.15",
    "xlsx": "^0.18.5"
  }
}