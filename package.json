{
  "name": "neonpro-monorepo",
  "version": "1.0.0",
  "description": "NeonPro aesthetic clinic management platform - Turborepo monorepo",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.22",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "esbuild": ">=0.25.0",
    "hono": ">=4.9.7",
    "tmp": ">=0.2.4",
    "form-data": ">=2.5.4",
    "d3-color": ">=3.1.0",
    "ws": ">=8.17.1",
    "cross-spawn": ">=6.0.6",
    "tar-fs": ">=2.1.3",
    "got": ">=11.8.5",
    "tough-cookie": ">=4.1.3",
    "cookie": ">=0.7.0",
    "bun": "^1.2.22"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --concurrency=20",
    "dev:web": "turbo dev --filter=@neonpro/web --concurrency=5",
    "dev:web:bun": "bun run --filter @neonpro/web dev",
    "test": "turbo test",
    "test:frontend": "bun run --filter @neonpro/tools-frontend-tests test",
    "test:frontend:comprehensive": "./tools/testing/frontend-testing.sh",
    "test:frontend:red": "./tools/testing/frontend-testing.sh --phase red",
    "test:frontend:green": "./tools/testing/frontend-testing.sh --phase green",
    "test:frontend:clinic": "./tools/testing/frontend-testing.sh --phase aesthetic-clinic",
    "test:frontend:refactor": "./tools/testing/frontend-testing.sh --phase refactor",
    "test:frontend:quality": "./tools/testing/frontend-testing.sh --phase quality",
    "test:frontend:report": "./tools/testing/frontend-testing.sh --phase report",
    "test:backend": "bun run --filter @neonpro/tools-backend-tests test",
    "test:database": "bun run --filter @neonpro/tools-database-tests test",
    "test:quality": "bun run --filter @neonpro/tools-quality-tests test",
    "test:orchestrate": "bun run --filter @neonpro/tools-orchestration orchestrate",
    "test:all-categories": "bun run test:orchestrate -- --all-categories",
    "test:healthcare": "bun run test:orchestrate -- --healthcare-compliance",
    "test:e2e": "bun run --filter @neonpro/tools-frontend-tests test:e2e",
    "test:a11y": "bun run --filter @neonpro/tools-frontend-tests test:a11y",
    "test:rls": "bun run --filter @neonpro/tools-database-tests test:rls",
    "test:compliance": "bun run --filter @neonpro/tools-database-tests test:compliance",
    "lint": "turbo lint",
    "lint:oxlint": "oxlint --config tools/quality/oxlint.config.mjs .",
    "lint:fix": "turbo lint:fix",
    "lint:oxlint:fix": "oxlint --fix --config tools/quality/oxlint.config.mjs .",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "turbo format",
    "format:check": "turbo format:check",
    "format:dprint": "dprint check --config tools/quality/dprint.json",
    "format:dprint:fix": "dprint fmt --config tools/quality/dprint.json",
    "test:quality": "bun run lint:oxlint && bun run format:dprint && bun run type-check",
    "test:quality:fix": "bun run lint:oxlint:fix && bun run format:dprint:fix",
    "test:quality:parallel": "./tools/quality/parallel-quality-check.sh",
    "test:quality:validate": "./tools/quality/ci-validation.sh",
    "compliance:check": "bun run pre-commit run --all-files",
    "compliance:install": "./scripts/install-hooks.sh",
    "healthcare:validate": "bun run lint:healthcare && bun run pre-commit run --all-files && bun run type-check",
    "deploy": "./scripts/deploy.sh",
    "deploy:prod": "./scripts/deploy.sh production",
    "deploy:preview": "./scripts/deploy.sh preview",
    "deploy:staging": "./scripts/deploy.sh staging",
    "vercel:build": "bun install && turbo build",
    "vercel:dev": "turbo dev",
    "vercel:deploy": "vercel deploy --prod --scope grupous-projects --yes",
    "vercel-build": "bun install && turbo build --filter=@neonpro/web",
    "prepare": "husky"
  },
  "keywords": [
    "neonpro",
    "healthcare",
    "aesthetic-clinic",
    "turborepo",
    "monorepo",
    "vite",
    "react",
    "typescript"
  ],
  "author": "NeonPro Development Team",
  "license": "MIT",
  "dependencies": {
    "@trpc/server": "^11.5.1",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "^5.0.50",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "ioredis": "^5.8.0",
    "lodash-es": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lovable-tagger": "^1.1.9",
    "tailwind-merge": "^3.3.1",
    "turbo": "^2.5.6",
    "valibot": "^1.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "bun": "^1.2.22",
    "dprint": "^0.49.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "fake-indexeddb": "^6.2.2",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "msw": "^2.11.2",
    "oxlint": "^1.15.0",
    "postcss": "^8.5.6",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.0",
    "rollup": "^4.52.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "supabase": "^2.40.7",
    "tailwindcss": "^4.1.13",
    "tar": "^6.2.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "undici": "^7.16.0",
    "vite": "^7.1.7",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "d3-color@<3.1.0": ">=3.1.0",
      "got@<11.8.5": ">=11.8.5",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "esbuild@<=0.24.2": ">=0.25.0",
      "cookie@<0.7.0": ">=0.7.0",
      "cross-spawn@<6.0.6": ">=6.0.6",
      "tar-fs@>=2.0.0 <2.1.2": ">=2.1.2",
      "form-data@<2.5.4": ">=2.5.4",
      "tmp@<=0.2.3": ">=0.2.4",
      "tar-fs@>=2.0.0 <2.1.3": ">=2.1.3"
    }
  },
  "main": "create_agent_tables_simple.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrupoUS/neonpro.git"
  },
  "bugs": {
    "url": "https://github.com/GrupoUS/neonpro/issues"
  },
  "homepage": "https://github.com/GrupoUS/neonpro#readme"
}
