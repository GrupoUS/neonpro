{
  "name": "neonpro-monorepo",
  "version": "1.0.0",
  "description": "NeonPro aesthetic clinic management platform - Turborepo monorepo",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.23",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "esbuild": ">=0.25.0",
    "hono": ">=4.9.7",
    "tmp": ">=0.2.4",
    "form-data": ">=2.5.4",
    "d3-color": ">=3.1.0",
    "ws": ">=8.17.1",
    "cross-spawn": ">=6.0.6",
    "tar-fs": ">=2.1.3",
    "got": ">=11.8.5",
    "tough-cookie": ">=4.1.3",
    "cookie": ">=0.7.0",
    "vite": "7.1.7",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  },
  "scripts": {
    "build": "turbo build",
    "build:bun": "turbo build --force",
    "build:deps": "turbo build:deps --filter=@neonpro/types --filter=@neonpro/config --concurrency=20",
    "build:core": "turbo build:types --filter=@neonpro/database --filter=@neonpro/core --concurrency=10",
    "build:ui": "turbo build:lib --filter=@neonpro/ui --concurrency=15",
    "build:apps": "turbo build --filter=@neonpro/web --filter=@neonpro/api --concurrency=5",
    "build:pipeline": "bun run build:deps && bun run build:core && bun run build:ui && bun run build:apps",
    "dev": "turbo dev --concurrency=15",
    "dev:web": "turbo dev --filter=@neonpro/web",
    "dev:api": "turbo dev --filter=neonpro-vercel-api",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "verify:edge": "bunx vite build --config apps/api/vite.config.ts",
    "route:parity": "bun scripts/check-route-parity.ts",
    "lint": "oxlint . --import-plugin --jsx-a11y-plugin",
    "lint:fix": "oxlint . --import-plugin --jsx-a11y-plugin --fix",
    "lint:security": "eslint . --max-warnings 0",
    "lint:security:fix": "eslint . --fix --max-warnings 0",
    "lint:format": "biome format .",
    "lint:format:fix": "biome format --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "format:check": "biome format .",
    "biome:check": "biome check .",
    "biome:fix": "biome check --fix .",
    "quality": "bun run lint && bun run lint:security && turbo type-check && bun run verify:edge",
    "quality:fix": "bun run lint:fix && bun run lint:security:fix",
    "quality:complete": "bun run lint && bun run lint:security && turbo type-check && bun run lint:format && bun run test:accessibility",
    "prepare": "husky",
    "test:healthcare-compliance": "vitest run --config=vitest.config.ts --packages=packages/healthcare-core/src/__tests__/ --timeout=30000",
    "test:security-compliance": "vitest run --config=vitest.config.ts --packages=packages/security/src/__tests__/ --timeout=30000",
    "test:regulatory-compliance": "vitest run --config=vitest.config.ts --packages=apps/api/src/__tests__/services/ --timeout=30000",
    "test:compliance-report": "vitest --timeout=30000 --coverage",
    "test:audit-compliance": "vitest --timeout=30000",
    "test:all-compliance": "vitest run --config=vitest.config.ts --timeout=30000",
    "test:accessibility": "playwright test --config=playwright.config.ts --project=accessibility",
    "test:healthcare": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/healthcare-results.json --config=vitest.config.ts",
    "test:security": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/security-results.json --config=vitest.config.ts",
    "test:performance": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/performance-results.json --config=vitest.config.ts",
    "test:compliance": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/compliance-results.json --config=vitest.config.ts",
    "test:baseline": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/baseline.json --config=vitest.config.ts",
    "test:parallel": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/parallel.json --pool=threads --maxWorkers=4 --config=vitest.config.ts",
    "test:isolate": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/isolate.json --isolate --config=vitest.config.ts",
    "test:memory": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/memory.json --reporter=memory --config=vitest.config.ts",
    "test:integration": "vitest --run --reporter=verbose --config-name=integration --config=vitest.config.ts",
    "test:api": "playwright test --config=playwright.config.ts --project=api",
    "test:mobile": "playwright test --config=playwright.config.ts --project=mobile",
    "test:perf": "vitest --timeout=30000 --isolate",
    "install:fresh": "rm -rf node_modules && bun install",
    "install:clean": "bun install --frozen-lockfile",
    "type-check:bun": "turbo type-check --force",
    "format:bun": "biome format . --write",
    "lint:bun": "oxlint . --import-plugin --jsx-a11y-plugin --fix",
    "lint:bun:security": "eslint . --fix --max-warnings 0",
    "lint:bun:format": "biome format --write .",
    "analyze": "turbo build && node scripts/analyze-bundle.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "perf:compare": "node scripts/compare-baselines.js",
    "migrate:scripts": "node scripts/migrate-root-scripts.js",
    "migrate:deps": "node scripts/migrate-dependencies.js",
    "setup:dev": "bun install && turbo build",
    "deploy:prepare": "turbo build && vitest && bun run lint",
    "health:check": "turbo type-check && vitest && bun run lint",
    "dev:hot": "turbo dev --concurrency=15 --force",
    "dev:inspect": "turbo dev --concurrency=10",
    "dev:bun": "turbo dev --concurrency=20",
    "build:bun": "turbo build --force",
    "perf:baseline": "node scripts/simple-benchmark.js",
    "perf:measure": "node scripts/benchmark-performance.js"
  },
  "keywords": [
    "neonpro",
    "healthcare",
    "aesthetic-clinic",
    "turborepo",
    "monorepo",
    "vite",
    "react",
    "typescript"
  ],
  "author": "NeonPro Development Team",
  "license": "MIT",
  "dependencies": {
    "@copilotkit/react-core": "^1.10.4",
    "@copilotkit/react-ui": "^1.10.4",
    "@hono/trpc-server": "0.4.0",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-progress": "1.1.7",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tabs": "1.1.13",
    "@tabler/icons-react": "3.35.0",
    "@trpc/server": "11.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "ai": "5.0.54",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dompurify": "3.2.7",
    "dotenv": "17.2.2",
    "framer-motion": "^11.0.0",
    "i18next": "25.5.2",
    "i18next-browser-languagedetector": "8.2.0",
    "ioredis": "5.8.0",
    "lodash-es": "4.17.21",
    "lodash.debounce": "4.0.8",
    "lovable-tagger": "1.1.10",
    "lucide-react": "0.544.0",
    "react-i18next": "16.0.0",
    "tailwind-merge": "3.3.1",
    "turbo": "2.5.8",
    "vite": "7.1.7",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@jest/globals": "30.1.2",
    "@playwright/test": "1.55.1",
    "@supabase/supabase-js": "2.58.0",
    "@tailwindcss/postcss": "4.1.13",
    "@testing-library/jest-dom": "6.8.0",
    "@types/jest": "30.0.0",
    "@types/jsdom": "21.1.7",
    "@types/lodash.debounce": "4.0.9",
    "@types/node": "24.5.2",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "@vitejs/plugin-react": "5.0.3",
    "@vitest/ui": "3.2.4",
    "autoprefixer": "10.4.21",
    "bun": "1.2.22",
    "dprint": "0.49.1",
    "eslint": "8.57.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-security": "2.1.1",
    "fake-indexeddb": "6.2.2",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "knip": "5.64.0",
    "msw": "2.11.3",
    "oxlint-tsgolint": "0.2.0",
    "postcss": "8.5.6",
    "pre-commit": "1.2.2",
    "prettier": "3.6.2",
    "rollup": "4.52.2",
    "rollup-plugin-visualizer": "6.0.3",
    "supabase": "2.45.5",
    "tailwindcss": "4.1.13",
    "tar": "6.2.1",
    "tsc-alias": "1.8.16",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "undici": "7.16.0",
    "vite-plugin-node-polyfills": "0.24.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "main": "create_agent_tables_simple.js",
  "directories": {
    "doc": "docs",
    "test": "tools/tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrupoUS/neonpro.git"
  },
  "bugs": {
