{
  "name": "neonpro-monorepo",
  "version": "1.0.0",
  "description": "NeonPro aesthetic clinic management platform - Turborepo monorepo",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.23",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "esbuild": ">=0.25.0",
    "hono": ">=4.9.7",
    "tmp": ">=0.2.4",
    "form-data": ">=2.5.4",
    "d3-color": ">=3.1.0",
    "ws": ">=8.17.1",
    "cross-spawn": ">=6.0.6",
    "tar-fs": ">=2.1.3",
    "got": ">=11.8.5",
    "tough-cookie": ">=4.1.3",
    "cookie": ">=0.7.0",
    "vite": "7.1.7",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  },
  "scripts": {
    "build": "node scripts/build-optimized.js",
    "build:healthcare": "node scripts/build-healthcare.js",
    "build:turbo": "bun run turbo build",
    "dev": "bun run turbo dev --concurrency=15",
    "dev:web": "bun run turbo dev --filter=@neonpro/web",
    "dev:api": "bun run turbo dev --filter=neonpro-vercel-api",
    "type-check": "bun run turbo type-check",
    "clean": "bun run turbo clean",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "oxlint . --import-plugin --react-plugin --jsx-a11y-plugin",
    "lint:fix": "oxlint . --import-plugin --react-plugin --jsx-a11y-plugin --fix",
    "lint:security": "eslint . --max-warnings 0",
    "lint:biome": "biome lint .",
    "lint:biome:fix": "biome lint --fix .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "format:check": "biome format .",
    "biome:check": "biome check .",
    "biome:fix": "biome check --fix .",
    "quality": "bun run lint && bun run lint:security && bun run type-check",
    "quality:fix": "bun run lint:fix && bun run lint:security",
    "quality:complete": "bun run lint && bun run lint:security && bun run type-check && bun run biome:check && bun run test:accessibility",
    "prepare": "husky",
    "test:healthcare-compliance": "bun test packages/healthcare-core/src/__tests__/ --timeout=30000",
    "test:security-compliance": "bun test packages/security/src/__tests__/ --timeout=30000",
    "test:regulatory-compliance": "bun test apps/api/src/__tests__/services/ --timeout=30000",
    "test:compliance-report": "bun test --timeout=30000 --coverage",
    "test:audit-compliance": "bun test --timeout=30000",
    "test:all-compliance": "bun run test:healthcare-compliance && bun run test:security-compliance && bun run test:regulatory-compliance",
    "test:accessibility": "bash scripts/accessibility-check.sh",
    "dev:bun": "turbo dev --concurrency=20",
    "build:bun": "turbo build --force",
    "test:bun": "vitest run --reporter=dot",
    "test:bun:fast": "vitest --config vitest.config.bun.ts --run",
    "test:bun:coverage": "vitest --config vitest.config.bun.ts --coverage",
    "test:bun:healthcare": "vitest --config vitest.config.bun.ts --run --reporter=verbose --reporter=json --outputFile=test-results/bun-healthcare-results.json",
    "test:bun:security": "vitest --config vitest.config.bun.ts --run --reporter=verbose --reporter=json --outputFile=test-results/bun-security-results.json",
    "test:bun:performance": "vitest --config vitest.config.bun.ts --run --reporter=verbose --reporter=json --outputFile=test-results/bun-performance-results.json",
    "test:bun:compliance": "vitest --config vitest.config.bun.ts --run --reporter=verbose --reporter=json --outputFile=test-results/bun-compliance-results.json",
    "test:bun:baseline": "vitest --config vitest.config.bun.ts --run --reporter=verbose --reporter=json --outputFile=test-results/bun-baseline.json",
    "test:bun:parallel": "vitest --config vitest.config.bun.ts --run --reporter=verbose --reporter=json --outputFile=test-results/bun-parallel.json --pool=threads --maxWorkers=4",
    "test:bun:isolate": "vitest --config vitest.config.bun.ts --run --reporter=verbose --reporter=json --outputFile=test-results/bun-isolate.json --isolate",
    "test:bun:memory": "vitest --config vitest.config.bun.ts --run --reporter=verbose --reporter=json --outputFile=test-results/bun-memory.json --reporter=memory",
    "test:bun:watch": "vitest --config vitest.config.bun.ts --watch",
    "test:bun:ui": "vitest --config vitest.config.bun.ts --ui",
    "test:bun:debug": "vitest --config vitest.config.bun.ts --run --reporter=verbose --no-coverage",
    "test:bun:integration": "vitest --config vitest.config.bun.ts --run --reporter=verbose --config-name=integration",
    "test:bun:e2e": "playwright test --config=playwright.config.ts --headed",
    "test:bun:mobile": "playwright test --config=playwright.config.ts --project=mobile",
    "test:bun:accessibility": "playwright test --config=playwright.config.ts --project=accessibility",
    "test:bun:api": "playwright test --config=playwright.config.ts --project=api",
    "test:native": "bun test --config bun.test.config.ts",
    "test:native:fast": "bun test --config bun.test.config.ts --timeout=5000",
    "test:native:coverage": "bun test --config bun.test.config.ts --coverage",
    "test:native:healthcare": "bun test --config bun.test.config.ts --test-name-pattern=\"healthcare\"",
    "test:native:lgpd": "bun test --config bun.test.config.ts --test-name-pattern=\"lgpd\"",
    "test:native:security": "bun test --config bun.test.config.ts --test-name-pattern=\"security\"",
    "test:native:parallel": "bun test --config bun.test.config.ts --max-concurrency=8",
    "test:native:isolate": "bun test --config bun.test.config.ts --concurrent",
    "test:native:memory": "bun test --config bun.test.config.ts --coverage --coverage-reporter=text",
    "test:perf": "bun test --timeout=30000 --isolate",
    "install:fresh": "rm -rf node_modules && bun install",
    "install:clean": "bun install --frozen-lockfile",
    "type-check:bun": "turbo type-check --force",
    "format:bun": "biome format . --write",
    "lint:bun": "oxlint . --import-plugin --react-plugin --jsx-a11y-plugin --fix",
    "analyze": "bun run build && bun run analyze:bundle",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "perf:compare": "node scripts/compare-baselines.js",
    "migrate:scripts": "node scripts/migrate-root-scripts.js",
    "migrate:deps": "node scripts/migrate-dependencies.js",
    "setup:dev": "bun install && bun run build",
    "deploy:prepare": "bun run build && bun run test:run && bun run lint",
    "health:check": "bun run type-check && bun run test:run && bun run lint",
    "dev:hot": "turbo dev --concurrency=15 --force",
    "dev:inspect": "turbo dev --concurrency=10",
    "perf:baseline": "node scripts/simple-benchmark.js",
    "perf:measure": "node scripts/benchmark-performance.js"
  },
  "keywords": [
    "neonpro",
    "healthcare",
    "aesthetic-clinic",
    "turborepo",
    "monorepo",
    "vite",
    "react",
    "typescript"
  ],
  "author": "NeonPro Development Team",
  "license": "MIT",
  "dependencies": {
    "@copilotkit/react-core": "^1.10.4",
    "@copilotkit/react-ui": "^1.10.4",
    "@hono/trpc-server": "0.4.0",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-progress": "1.1.7",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tabs": "1.1.13",
    "@tabler/icons-react": "3.35.0",
    "@trpc/server": "11.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "ai": "5.0.54",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dompurify": "3.2.7",
    "dotenv": "17.2.2",
    "framer-motion": "^11.0.0",
    "i18next": "25.5.2",
    "i18next-browser-languagedetector": "8.2.0",
    "ioredis": "5.8.0",
    "lodash-es": "4.17.21",
    "lodash.debounce": "4.0.8",
    "lovable-tagger": "1.1.10",
    "lucide-react": "0.544.0",
    "react-i18next": "16.0.0",
    "tailwind-merge": "3.3.1",
    "turbo": "2.5.8",
    "vite": "7.1.7",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@jest/globals": "30.1.2",
    "@playwright/test": "1.55.1",
    "@supabase/supabase-js": "2.58.0",
    "@tailwindcss/postcss": "4.1.13",
    "@testing-library/jest-dom": "6.8.0",
    "@types/jest": "30.0.0",
    "@types/jsdom": "21.1.7",
    "@types/lodash.debounce": "4.0.9",
    "@types/node": "24.5.2",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "@vitejs/plugin-react": "5.0.3",
    "@vitest/ui": "3.2.4",
    "autoprefixer": "10.4.21",
    "bun": "1.2.22",
    "dprint": "0.49.1",
    "eslint": "8.57.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-security": "2.1.1",
    "fake-indexeddb": "6.2.2",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "knip": "5.64.0",
    "msw": "2.11.3",
    "oxlint-tsgolint": "0.2.0",
    "postcss": "8.5.6",
    "pre-commit": "1.2.2",
    "prettier": "3.6.2",
    "rollup": "4.52.2",
    "rollup-plugin-visualizer": "6.0.3",
    "supabase": "2.45.5",
    "tailwindcss": "4.1.13",
    "tar": "6.2.1",
    "tsc-alias": "1.8.16",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "undici": "7.16.0",
    "vite-plugin-node-polyfills": "0.24.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "main": "create_agent_tables_simple.js",
  "directories": {
    "doc": "docs",
    "test": "tools/tests"
  },
  "repository": {
    "type": "git",
