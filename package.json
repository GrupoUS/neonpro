{
  "name": "neonpro-monorepo",
  "version": "1.0.0",
  "description": "NeonPro aesthetic clinic management platform - Turborepo monorepo",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.22",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "esbuild": ">=0.25.0",
    "hono": ">=4.9.7",
    "tmp": ">=0.2.4",
    "form-data": ">=2.5.4",
    "d3-color": ">=3.1.0",
    "ws": ">=8.17.1",
    "cross-spawn": ">=6.0.6",
    "tar-fs": ">=2.1.3",
    "got": ">=11.8.5",
    "tough-cookie": ">=4.1.3",
    "cookie": ">=0.7.0",
    "bun": "^1.2.22"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --concurrency=20",
    "dev:web": "turbo dev --filter=@neonpro/web --concurrency=5",
    "dev:web:bun": "bun run --filter @neonpro/web dev",
    "test": "cd tools/tests-consolidated && bun run test",
    "test:unit": "cd tools/tests-consolidated && bun run test:unit",
    "test:integration": "cd tools/tests-consolidated && bun run test:integration",
    "test:e2e": "cd tools/tests-consolidated && bun run test:e2e",
    "test:watch": "cd tools/tests-consolidated && bun run test:watch",
    "test:coverage": "cd tools/tests-consolidated && bun run test:coverage",
    "test:validate": "cd tools/tests-consolidated && bun run validate",
    "test:ci": "cd tools/tests-consolidated && bun run ci",
    "lint": "oxlint",
    "lint:oxlint": "oxlint",
    "lint:fix": "oxlint --fix",
    "lint:oxlint:fix": "oxlint --fix",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "dprint fmt",
    "format:check": "dprint check",
    "format:dprint": "dprint check",
    "format:dprint:fix": "dprint fmt",
    "test:quality:fix": "bun run lint:oxlint:fix && bun run format:dprint:fix",
    "test:quality:parallel": "./tools/quality/parallel-quality-check.sh",
    "test:quality:validate": "./tools/quality/ci-validation.sh",
    "compliance:check": "bun run pre-commit run --all-files",
    "compliance:install": "./scripts/install-hooks.sh",
    "healthcare:validate": "bun run lint:healthcare && bun run pre-commit run --all-files && bun run type-check",
    "deploy": "./scripts/deploy.sh",
    "deploy:prod": "./scripts/deploy.sh production",
    "deploy:preview": "./scripts/deploy.sh preview",
    "deploy:staging": "./scripts/deploy.sh staging",
    "vercel:build": "bun install && turbo build",
    "vercel:dev": "turbo dev",
    "vercel:deploy": "vercel deploy --prod --scope grupous-projects --yes",
    "vercel-build": "bun install && turbo build --filter=@neonpro/web",
    "build:vercel": "echo 'Building NeonPro for Vercel...' && cd apps/web && bun install && cd ../.. && turbo build --filter=@neonpro/web && mkdir -p dist && if [ -f apps/web/dist/index.html ]; then echo 'Copying React build...' && cp -r apps/web/dist/* dist/; else echo 'Creating fallback page...' && echo '<!DOCTYPE html><html><head><title>NeonPro</title><meta charset=\"UTF-8\"><meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\"><style>body{font-family:system-ui;text-align:center;padding:2rem;background:linear-gradient(135deg,#667eea,#764ba2);color:white;min-height:100vh;display:flex;align-items:center;justify-content:center}</style></head><body><div><h1>üè• NeonPro</h1><p>Healthcare Platform for Brazilian Aesthetic Clinics</p><p>üöÄ Deploy realizado com sucesso!</p><p>Monorepo Turborepo + React + Vite + TypeScript + Bun</p></div></body></html>' > dist/index.html; fi && echo 'Build completed!' && find dist -type f | head -10",
    "prepare": "husky"
  },
  "keywords": [
    "neonpro",
    "healthcare",
    "aesthetic-clinic",
    "turborepo",
    "monorepo",
    "vite",
    "react",
    "typescript"
  ],
  "author": "NeonPro Development Team",
  "license": "MIT",
  "dependencies": {
    "@hono/trpc-server": "^0.4.0",
    "@trpc/server": "^11.5.1",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "^5.0.50",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.7",
    "dotenv": "^17.2.2",
    "ioredis": "^5.8.0",
    "lodash-es": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lovable-tagger": "^1.1.9",
    "tailwind-merge": "^3.3.1",
    "turbo": "^2.5.6",
    "valibot": "^1.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "bun": "^1.2.22",
    "dprint": "^0.49.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^2.1.0",
    "fake-indexeddb": "^6.2.2",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "knip": "^5.64.0",
    "msw": "^2.11.2",
    "oxlint": "^1.17.0",
    "oxlint-tsgolint": "^0.2.0",
    "postcss": "^8.5.6",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.0",
    "rollup": "^4.52.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "supabase": "^2.40.7",
    "tailwindcss": "^4.1.13",
    "tar": "^6.2.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "undici": "^7.16.0",
    "vite": "^7.1.7",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "d3-color@<3.1.0": ">=3.1.0",
      "got@<11.8.5": ">=11.8.5",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "esbuild@<=0.24.2": ">=0.25.0",
      "cookie@<0.7.0": ">=0.7.0",
      "cross-spawn@<6.0.6": ">=6.0.6",
      "tar-fs@>=2.0.0 <2.1.2": ">=2.1.2",
      "form-data@<2.5.4": ">=2.5.4",
      "tmp@<=0.2.3": ">=0.2.4",
      "tar-fs@>=2.0.0 <2.1.3": ">=2.1.3"
    }
  },
  "main": "create_agent_tables_simple.js",
  "directories": {
    "doc": "docs",
    "test": "tools/tests-consolidated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrupoUS/neonpro.git"
  },
  "bugs": {
    "url": "https://github.com/GrupoUS/neonpro/issues"
  },
  "homepage": "https://github.com/GrupoUS/neonpro#readme"
}
