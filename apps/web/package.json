{
  "name": "@neonpro/web",
  "version": "1.0.0",
  "description": "NeonPro Web Application - Modern healthcare platform for Brazilian aesthetic clinics",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx vite",
    "build": "bunx vite build",
    "build:check": "bun run type-check && bunx vite build",
    "preview": "bunx vite preview",
    "type-check": "tsc --noEmit",
    "test": "bunx vitest run",
    "test:watch": "bunx vitest",
    "test:e2e": "bunx playwright test",
    "lint": "bunx oxlint -c ../../.oxlintrc.json",
    "lint:fix": "bunx oxlint -c ../../.oxlintrc.json --fix",
    "format": "bunx dprint fmt",
    "format:check": "bunx dprint check",
    "validate": "bun run format:check && bun run lint && bun run type-check && bun run test"
  },
