{
  "name": "@neonpro/web",
  "version": "1.0.0",
  "description": "NeonPro Web Application - Modern healthcare platform for Brazilian aesthetic clinics",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build --mode production",
    "build:vercel": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:unit": "vitest --config vitest.unit.config.ts",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "test:healthcare": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/healthcare-test-results.json",
    "test:lgpd": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/lgpd-compliance-results.json",
    "test:performance": "vitest --run --reporter=verbose --reporter=json --outputFile=test-results/performance-test-results.json",
    "test:ci": "vitest --run --coverage --reporter=json --outputFile=test-results/ci-test-results.json",
    "lint": "oxlint --config ../../.oxlintrc.json",
    "lint:fix": "oxlint --fix --config ../../.oxlintrc.json",
    "format": "dprint fmt",
    "format:check": "dprint check",
    "clean": "rm -rf coverage reports node_modules/.cache test-results",
    "validate": "pnpm run format:check && pnpm run lint && pnpm run test:run",
    "validate:healthcare": "pnpm run format:check && pnpm run lint && pnpm run test:healthcare && pnpm run test:lgpd"
  },
  "dependencies": {
    "@copilotkit/react-core": "^1.10.4",
    "@copilotkit/react-ui": "^1.10.4",
    "@copilotkit/runtime-client-gql": "^1.10.4",
    "@copilotkit/shared": "^1.10.4",
    "@neonpro/shared": "workspace:*",
    "@neonpro/types": "workspace:*",
    "@neonpro/utils": "workspace:*",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.3",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.1",
    "@tanstack/react-router": "^1.132.7",
    "@trpc/client": "^11.5.1",
    "@trpc/react-query": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.460.0",
    "msw": "^2.11.3",
    "react": "^19.0.1",
    "react-dom": "^19.0.1",
    "react-hook-form": "^7.62.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "valibot": "^1.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.90.1",
    "@tanstack/router-plugin": "^1.132.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dprint": "^0.45.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jsdom": "^27.0.0",
    "oxlint": "^0.2.0",
    "playwright": "^1.40.0",
    "typescript": "^5.9.2",
    "vite": "^5.4.20",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "workspaces": [
    "."
  ]
}
