{
  "name": "@neonpro/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./performance/lighthouse-report.json --chrome-flags=\"--headless\" --quiet",
    "performance:analyze": "node scripts/performance/bundle-analyzer.js",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:hooks": "jest --testPathPattern=__tests__/hooks",
    "test:components": "jest --testPathPattern=__tests__/components",
    "test:api": "jest --testPathPattern=__tests__/api",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "test:e2e:open": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "test:e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "test:component": "cypress open --component",
    "test:component:headless": "cypress run --component",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist out"
  },
  "dependencies": {
    "@neonpro/ui": "1.0.0",
    "@neonpro/utils": "1.0.0",
    "@neonpro/types": "1.0.0",
    "@ai-sdk/openai": "^0.0.66",
    "@auth/supabase-adapter": "^1.6.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@faker-js/faker": "^9.0.3",
    "@hookform/resolvers": "^3.10.0",
    "@next/bundle-analyzer": "^15.1.0",
    "@prisma/client": "^5.21.1",
    "@sentry/nextjs": "^8.46.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.46.1",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@types/bcryptjs": "^2.4.6",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.14",
    "ai": "^3.4.33",
    "bcryptjs": "^2.4.3",
    "cmdk": "^1.0.0",
    "date-fns-tz": "^3.2.0",
    "embla-carousel-react": "^8.3.1",
    "framer-motion": "^11.14.4",

    "next": "^15.1.0",
    "next-auth": "^4.24.10",
    "next-safe-action": "^7.9.8",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-day-picker": "^9.4.4",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.13.3",
    "server-only": "^0.0.1",
    "sonner": "^1.7.0",
    "vaul": "^1.1.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.16.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.2.1",
    "playwright": "^1.49.1",
    "postcss": "^8.5.1",
    "prisma": "^5.21.1",
    "start-server-and-test": "^2.0.9",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  }
}
