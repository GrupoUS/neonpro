{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "target": "es2015",
    "lib": [
      "dom",
      "dom.iterable",
      "es2021"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "outDir": "dist",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "@/styles/*": [
        "./styles/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/utils/*": [
        "./utils/*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@neonpro/ui": [
        "../../packages/ui/src"
      ],
      "@neonpro/ui/*": [
        "../../packages/ui/src/*"
      ],
      "@neonpro/ai": [
        "../../packages/ai/src"
      ],
      "@neonpro/ai/*": [
        "../../packages/ai/src/*"
      ],
      "@neonpro/types": [
        "../../packages/types/src"
      ],
      "@neonpro/types/*": [
        "../../packages/types/src/*"
      ],
      "@neonpro/utils": [
        "../../packages/utils/src"
      ],
      "@neonpro/utils/*": [
        "../../packages/utils/src/*"
      ],
      "@neonpro/domain": [
        "../../packages/domain/src"
      ],
      "@neonpro/domain/*": [
        "../../packages/domain/src/*"
      ],
      "@neonpro/core-services": [
        "../../packages/core-services/src"
      ],
      "@neonpro/core-services/*": [
        "../../packages/core-services/src/*"
      ],
      "@neonpro/compliance": [
        "../../packages/compliance/src"
      ],
      "@neonpro/compliance/*": [
        "../../packages/compliance/src/*"
      ],
      "@neonpro/security": [
        "../../packages/security/src"
      ],
      "@neonpro/security/*": [
        "../../packages/security/src/*"
      ],
      "@neonpro/auth": [
        "../../packages/auth/src"
      ],
      "@neonpro/auth/*": [
        "../../packages/auth/src/*"
      ],
      "@neonpro/database": [
        "../../packages/database/src"
      ],
      "@neonpro/database/*": [
        "../../packages/database/src/*"
      ],
      "@neonpro/cache": [
        "../../packages/cache/src"
      ],
      "@neonpro/cache/*": [
        "../../packages/cache/src/*"
      ],
      "@neonpro/monitoring": [
        "../../packages/monitoring/src"
      ],
      "@neonpro/monitoring/*": [
        "../../packages/monitoring/src/*"
      ],
      "@neonpro/enterprise": [
        "../../packages/enterprise/src"
      ],
      "@neonpro/enterprise/*": [
        "../../packages/enterprise/src/*"
      ],
      "qrcode.react": [
        "./lib/placeholders/qrcode-react"
      ]
    },
    "emitDeclarationOnly": true,
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".next/types/**/*",
    "__tests__/**/*",
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "references": [
    {
      "path": "../../packages/ui"
    },
    {
      "path": "../../packages/ai"
    },
    {
      "path": "../../packages/types"
    },
    {
      "path": "../../packages/utils"
    },
    {
      "path": "../../packages/domain"
    },
    {
      "path": "../../packages/core-services"
    },
    {
      "path": "../../packages/compliance"
    },
    {
      "path": "../../packages/database"
    },
    {
      "path": "../../packages/shared"
    }
  ]
}
