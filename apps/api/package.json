{
  "name": "@neonpro/api",
  "version": "1.0.0",
  "description": "NeonPro API - Hono.dev backend for aesthetic clinics management",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch --env-file=.env src/index.ts",
    "dev:api": "NODE_ENV=development PORT=3004 tsx --env-file=.env src/index.ts",
    "build": "tsup src/index.ts --format esm --target esnext --clean",
    "start": "node dist/index.js",
    "prepare": "echo 'prepare script temporarily disabled for workspace install'",
    "postinstall": "prisma generate --schema ../../packages/database/prisma/schema.prisma",
    "type-check": "tsc -p tsconfig.feature.json --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:contract": "vitest --run tests/contract",
    "coverage": "vitest --run --coverage",
    "test:coverage": "vitest --run --coverage",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "dprint fmt",
    "prisma:generate": "prisma generate --schema ../../packages/database/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema ../../packages/database/prisma/schema.prisma"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.15",
    "@ai-sdk/google": "^2.0.13",
    "@ai-sdk/openai": "^2.0.13",
    "@hono/node-server": "^1.19.2",
    "@hono/swagger-ui": "^0.4.1",
    "@hono/zod-openapi": "^0.18.4",
    "@hono/zod-validator": "^0.2.2",
    "@neonpro/security": "0.1.0",
    "@neonpro/shared": "0.1.0",
    "@neonpro/types": "0.1.0",
    "@prisma/client": "^5.22.0",
    "@sentry/profiling-node": "^10.12.0",
    "@supabase/supabase-js": "^2.57.4",
    "ai": "^5.0.39",
    "bcryptjs": "^3.0.2",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.2",
    "hono": "^4.9.7",
    "jose": "^5.10.0",
    "nanoid": "^5.1.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@sentry/node": "^8.55.0",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.13",
    "@types/supertest": "^6.0.0",
    "prisma": "^5.22.0",
    "rollbar": "^2.26.4",
    "supertest": "^6.3.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "schema": "../../packages/database/prisma/schema.prisma"
  },
  "keywords": [
    "neonpro",
    "aesthetic-clinics",
    "healthcare",
    "hono",
    "typescript",
    "supabase",
    "prisma",
    "lgpd",
    "anvisa"
  ],
  "author": "NeonPro Team",
  "license": "PROPRIETARY"
}
