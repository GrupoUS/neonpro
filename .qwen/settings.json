{
    "mcpServers": {
        "shadcn": {
            "command": "bunx",
            "args": [
                "shadcn@latest",
                "mcp"
            ]
        },
        "desktop-commander": {
            "command": "bunx",
            "args": [
                "@wonderwhy-er/desktop-commander@latest"
            ],
            "env": {
                "MAX_MCP_OUTPUT_TOKENS": "20000"
            },
            "alwaysAllow": [
                "start_process",
                "get_config",
                "set_config_value",
                "read_file",
                "read_multiple_files",
                "write_file",
                "create_directory",
                "list_directory",
                "move_file",
                "start_search",
                "get_more_search_results",
                "stop_search",
