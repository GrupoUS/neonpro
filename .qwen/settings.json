                "edit_block",
                "read_process_output",
                "interact_with_process",
                "force_terminate",
                "list_sessions",
                "list_processes",
                "kill_process",
                "search_files"
            ],
            "disabledTools": [
                "give_feedback_to_desktop_commander",
                "get_usage_stats"
            ]
        },
        "Tavily": {
            "command": "bunx",
            "args": [
                "tavily-mcp@latest"
