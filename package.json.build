{
  "name": "neonpro-monorepo-enhanced",
  "version": "1.0.0",
  "description": "NeonPro aesthetic clinic management platform - Enhanced build system for 8-package architecture",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.22",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "node scripts/build-system.js build",
    "build:core": "node scripts/build-system.js build:packages",
    "build:packages": "node scripts/build-system.js build:packages",
    "build:apps": "node scripts/build-system.js build:apps",
    "build:production": "node scripts/build-system.js build:production",
    "build:types": "node scripts/build-system.js build --filter=@neonpro/types",
    "build:shared": "node scripts/build-system.js build --filter=@neonpro/shared",
    "build:database": "node scripts/build-system.js build --filter=@neonpro/database",
    "build:ai-services": "node scripts/build-system.js build --filter=@neonpro/ai-services",
    "build:healthcare-core": "node scripts/build-system.js build --filter=@neonpro/healthcare-core",
    "build:security-compliance": "node scripts/build-system.js build --filter=@neonpro/security-compliance",
    "build:api-gateway": "node scripts/build-system.js build --filter=@neonpro/api-gateway",
    "build:ui": "node scripts/build-system.js build --filter=@neonpro/ui",
    "dev": "node scripts/dev-server.js start",
    "dev:web": "node scripts/dev-server.js start --packages @neonpro/web",
    "dev:api": "node scripts/dev-server.js start --packages @neonpro/api",
    "dev:all": "node scripts/dev-server.js start",
    "test": "node scripts/testing-infrastructure.js test:all",
    "test:unit": "node scripts/testing-infrastructure.js test:unit",
    "test:integration": "node scripts/testing-infrastructure.js test:integration",
    "test:e2e": "node scripts/testing-infrastructure.js test:e2e",
    "test:performance": "node scripts/testing-infrastructure.js test:performance",
    "test:security": "node scripts/testing-infrastructure.js test:security",
    "test:coverage": "node scripts/testing-infrastructure.js coverage",
    "test:watch": "node scripts/testing-infrastructure.js test --watch",
    "type-check": "node scripts/build-system.js type-check",
    "lint": "node scripts/build-system.js lint",
    "lint:fix": "node scripts/build-system.js lint:fix",
    "lint:oxlint": "node scripts/build-system.js lint",
    "lint:oxlint:fix": "node scripts/build-system.js lint:fix",
    "quality": "node scripts/build-system.js quality",
    "quality:strict": "node scripts/build-system.js quality --strict",
    "clean": "node scripts/build-system.js clean",
    "clean:deep": "node scripts/build-system.js clean --deep",
    "format": "node scripts/build-system.js format",
    "format:check": "node scripts/build-system.js format --check",
    "format:fix": "dprint fmt",
    "health": "node scripts/build-system.js health",
    "status": "node scripts/dev-server.js status",
    "deploy": "node scripts/deployment.js deploy",
    "deploy:staging": "node scripts/deployment.js deploy staging",
    "deploy:production": "node scripts/deployment.js deploy production",
    "deploy:package": "node scripts/deployment.js package",
    "deploy:health": "node scripts/deployment.js health",
    "deploy:backup": "node scripts/deployment.js backup",
    "deploy:rollback": "node scripts/deployment.js rollback",
    "deploy:status": "node scripts/deployment.js status",
    "dev-tooling": "node scripts/dev-tooling.js",
    "analyze": "node scripts/dev-tooling.js analyze",
    "analyze:code": "node scripts/dev-tooling.js analyze --packages",
    "analyze:performance": "node scripts/dev-tooling.js analyze --performance",
    "analyze:security": "node scripts/dev-tooling.js analyze --security",
    "generate": "node scripts/dev-tooling.js generate",
    "generate:component": "node scripts/dev-tooling.js generate component",
    "generate:service": "node scripts/dev-tooling.js generate service",
    "generate:test": "node scripts/dev-tooling.js generate test",
    "monitor": "node scripts/dev-tooling.js monitor",
    "metrics": "node scripts/dev-tooling.js metrics",
    "deps:update": "node scripts/dev-tooling.js deps update",
    "deps:audit": "node scripts/dev-tooling.js deps audit",
    "deps:dedupe": "node scripts/dev-tooling.js deps dedupe",
    "matrix": "node scripts/build-system.js matrix",
    "report": "node scripts/testing-infrastructure.js report",
    "report:deployment": "node scripts/deployment.js report",
    "report:analysis": "node scripts/dev-tooling.js analyze --html",
    "setup:dev": "node scripts/dev-server.js setup",
    "setup:test": "node scripts/testing-infrastructure.js init",
    "setup:debug": "node scripts/dev-tooling.js debug",
    "verify": "node scripts/build-system.js health && node scripts/build-system.js type-check && node scripts/build-system.js lint && node scripts/testing-infrastructure.js test:unit"
  },
  "dependencies": {
    "@hono/trpc-server": "^0.4.0",
