    "postcss": "^8.5.6",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.0",
    "rollup": "^4.52.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "supabase": "^2.40.7",
    "tailwindcss": "^4.1.13",
    "tar": "^6.2.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "undici": "^7.16.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "esbuild": ">=0.25.0",
    "hono": ">=4.9.7",
    "tmp": ">=0.2.4",
