{
  "version": 2,
  "buildCommand": "npx turbo build --filter=@neonpro/web",
  "outputDirectory": "apps/web/dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}