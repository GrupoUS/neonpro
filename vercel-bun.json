{
  "version": 2,
  "buildCommand": "cd apps/web && bun install && bun run build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "bun install",
  "framework": null,
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}