{
  "name": "@neonpro/cache",
  "version": "0.1.0",
  "private": true,
  "description": "NeonPro Healthcare Caching Utilities - LGPD Compliant Performance Optimization",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
