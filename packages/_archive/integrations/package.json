{
  "name": "@neonpro/integrations",
  "version": "0.1.0",
  "description": "Integration Ecosystem for Healthcare Systems",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --outDir dist --dts",
    "dev": "tsc --watch",
    "lint": "oxlint src/",
    "format": "dprint fmt",
    "typecheck": "tsc --noEmit",
    "test": "pnpm -w --filter @neonpro/tooling vitest run -- --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@neonpro/database": "workspace:*",
    "@supabase/supabase-js": "^2.45.4",
    "zod": "^3.23.8",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "healthcare",
    "integration",
    "ehr",
    "hl7",
    "fhir",
    "lgpd",
    "anvisa"
  ],
  "author": "NeonPro Healthcare Team",
  "license": "PROPRIETARY",
  "private": true
}
