{
  "name": "@neonpro/ai",
  "version": "0.1.0",
  "private": true,
  "description": "NeonPro AI Healthcare Features - Constitutional AI Ethics with ≥95% Medical Accuracy",
  "keywords": [
    "healthcare",
    "artificial-intelligence",
    "medical-ai",
    "constitutional-ethics",
    "explainable-ai",
    "treatment-prediction",
    "patient-risk-assessment",
    "intelligent-scheduling",
    "healthcare-chatbot",
    "medical-accuracy"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./scheduling": {
      "types": "./dist/scheduling/index.d.ts",
      "import": "./dist/scheduling/index.js",
      "require": "./dist/scheduling/index.js"
    },
    "./prediction": {
      "types": "./dist/prediction/index.d.ts",
      "import": "./dist/prediction/index.js",
      "require": "./dist/prediction/index.js"
    },
    "./risk-assessment": {
      "types": "./dist/risk-assessment/index.d.ts",
      "import": "./dist/risk-assessment/index.js",
      "require": "./dist/risk-assessment/index.js"
    },
    "./recommendations": {
      "types": "./dist/recommendations/index.d.ts",
      "import": "./dist/recommendations/index.js",
      "require": "./dist/recommendations/index.js"
    },
    "./chatbot": {
      "types": "./dist/chatbot/index.d.ts",
      "import": "./dist/chatbot/index.js",
      "require": "./dist/chatbot/index.js"
    },
    "./ethics": {
      "types": "./dist/ethics/index.d.ts",
      "import": "./dist/ethics/index.js",
      "require": "./dist/ethics/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js"
    }
  },  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "biome lint src --write",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:medical-accuracy": "vitest --testNamePattern=\"medical.*accuracy|prediction.*accuracy|≥95\"",
    "validate:ai-ethics": "node scripts/validate-ai-ethics.js",
    "validate:medical-accuracy": "node scripts/validate-medical-accuracy.js",
    "test:constitutional-ai": "vitest --testNamePattern=\"constitutional|ethics|explainable\"",
    "benchmark:accuracy": "node scripts/benchmark-accuracy.js",
    "audit:ai-compliance": "node scripts/ai-compliance-audit.js"
  },
  "dependencies": {
    "@neonpro/compliance": "workspace:*",
    "@neonpro/db": "workspace:*",
    "@neonpro/domain": "workspace:*",
    "@neonpro/types": "workspace:*",
    "@neonpro/utils": "workspace:*",
    "@neonpro/devops": "workspace:*",
    "openai": "^4.52.7",
    "@azure/openai": "^1.0.0-beta.12",
    "langchain": "^0.3.2",
    "@langchain/openai": "^0.3.0",
    "zod": "^3.22.4",
    "date-fns": "^3.0.6",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "ml-matrix": "^6.10.9",
    "simple-statistics": "^7.8.3",
    "tensorflow": "^4.17.0",
    "@tensorflow/tfjs-node": "^4.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/lodash": "^4.17.7",
    "@types/uuid": "^9.0.7",
    "@types/crypto-js": "^4.2.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "next": "^15.0.0",
    "@supabase/supabase-js": "^2.55.0"
  }
}