{
  "name": "@neonpro/ai-services",
  "version": "2.3.0",
  "type": "module",
  "description": "Unified AI provider management system with healthcare compliance for NeonPro platform",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun x tsc -b",
    "dev": "tsc -w -p tsconfig.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "test": "bun test run",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "validate-providers": "tsx scripts/validate-providers.ts",
    "test-clinical-ai": "bun test src/services/clinical-ai.test.ts",
    "health-check": "tsx scripts/health-check.ts",
    "compliance-check": "tsx scripts/compliance-check.ts"
  },
  "dependencies": {
    "@neonpro/utils": "workspace:*",
    "@anthropic-ai/sdk": "^0.63.1",
    "@google/generative-ai": "^0.24.1",
    "openai": "^4.0.0",
    "zod": "^4.1.11",
    "ai": "^5.0.50",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "@types/ioredis": "^5.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "tsx": "^4.6.2",
    "rimraf": "^5.0.1"
  },
  "peerDependencies": {
    "@neonpro/security": "workspace:*",
    "@neonpro/database": "workspace:*"
  },
  "optionalDependencies": {
    "@neonpro/security": "workspace:*",
    "@neonpro/database": "workspace:*"
  },
  "files": [
    "dist",
    "src/**/*"
  ],
  "keywords": [
    "ai",
    "machine-learning",
    "healthcare",
    "clinical-ai",
    "decision-support",
    "medical-ai",
    "providers",
    "anthropic",
    "openai",
    "google",
    "unified",
    "healthcare-compliance",
    "lgpd",
    "anvisa",
    "brazil"
  ],
  "author": "NeonPro Team",
  "license": "MIT",
  "publishConfig": {
    "access": "restricted"
  }
}