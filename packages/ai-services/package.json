{
  "name": "@neonpro/ai-services",
  "version": "1.0.0",
  "description": "AI provider management and clinical decision support for NeonPro healthcare platform",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    },
    "./providers/*": {
      "types": "./dist/src/providers/*.d.ts",
      "import": "./dist/src/providers/*.js",
      "require": "./dist/src/providers/*.js"
    },
    "./services/*": {
      "types": "./dist/src/services/*.d.ts",
      "import": "./dist/src/services/*.js",
      "require": "./dist/src/services/*.js"
    }
  },
  "scripts": {
    "build": "bun x tsc -b",
    "dev": "tsc -w -p tsconfig.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "test": "bun test run",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "validate-providers": "tsx scripts/validate-providers.ts",
    "test-clinical-ai": "bun test src/services/clinical-ai.test.ts"
  },
  "dependencies": {
    "@neonpro/types": "workspace:*",
    "@neonpro/shared": "workspace:*",
    "@anthropic-ai/sdk": "^0.63.1",
    "@google/generative-ai": "^0.24.1",
    "openai": "^4.0.0",
    "zod": "^4.1.11",
    "ai": "^5.0.50"
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "tsx": "^4.6.2"
  },
  "peerDependencies": {
    "@neonpro/security-compliance": "workspace:*",
    "@neonpro/database": "workspace:*"
  },
  "files": [
    "dist",
    "src/**/*"
  ],
  "keywords": [
    "ai",
    "machine-learning",
    "healthcare",
    "clinical-ai",
    "decision-support",
    "medical-ai",
    "providers",
    "anthropic",
    "openai",
    "google"
  ],
  "author": "NeonPro Team",
  "license": "MIT",
  "publishConfig": {
    "access": "restricted"
  }
}