{
  "name": "@neonpro/types",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.js"
    },
    "./domain": {
      "types": "./dist/domain/index.d.ts",
      "import": "./dist/domain/index.js",
      "require": "./dist/domain/index.js"
    },
    "./validation": {
      "types": "./dist/validation/index.d.ts",
      "import": "./dist/validation/index.js",
      "require": "./dist/validation/index.js"
    },
    "./domain/*": {
      "types": "./dist/domain/*.d.ts",
      "import": "./dist/domain/*.js",
      "require": "./dist/domain/*.js"
    },
    "./validation/*": {
      "types": "./dist/validation/*.d.ts",
      "import": "./dist/validation/*.js",
      "require": "./dist/validation/*.js"
    },
    "./api/*": {
      "types": "./dist/api/*.d.ts",
      "import": "./dist/api/*.js",
      "require": "./dist/api/*.js"
    },
    "./api/contracts": {
      "types": "./dist/api/contracts.d.ts",
      "import": "./dist/api/contracts.js",
      "require": "./dist/api/contracts.js"
    },
    "./governance.types": {
      "types": "./dist/governance.types.d.ts",
      "import": "./dist/governance.types.js",
      "require": "./dist/governance.types.js"
    },
    "./healthcare-governance.types": {
      "types": "./dist/healthcare-governance.types.d.ts",
      "import": "./dist/healthcare-governance.types.js",
      "require": "./dist/healthcare-governance.types.js"
    }
  },
  "scripts": {
    "build": "bun x tsc -b",
    "dev": "bun x tsc -w -p tsconfig.json",
    "type-check": "bun x tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "test": "bun test",
    "build:scheduling": "tsc -p tsconfig.scheduling.json",
    "test:watch": "bun test --watch",
    "lint": "echo 'lint placeholder'",
    "prepare": "echo 'prepare script disabled for workspace install'"
  },
  "dependencies": {
    "zod": "^3.25.76",
    "valibot": "^1.1.0",
    "@trpc/server": "^11.5.1"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
