{
  "name": "@neonpro/types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for NeonPro aesthetic clinic platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./database": {
      "import": "./dist/database.js",
      "types": "./dist/database.d.ts"
    },
    "./healthcare": {
      "import": "./dist/healthcare.js",
      "types": "./dist/healthcare.d.ts"
    },
    "./common": {
      "import": "./dist/common.js",
      "types": "./dist/common.d.ts"
    }
  },
  "scripts": {
    "build": "bun ../../scripts/tsc-wrapper.cjs",
    "dev": "bun ../../scripts/tsc-wrapper.cjs --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "types:generate": "supabase gen types typescript --local > src/generated/database.ts",
    "dev:bun": "vite dev --force",
    "build:bun": "bun ../../scripts/tsc-wrapper.cjs --build --declaration",
    "build:analyze": "vite build --mode analyze",
    "test:bun": "vitest run --reporter=dot",
    "test:watch:bun": "vitest watch --reporter=dot",
    "type-check:bun": "bun ../../scripts/tsc-wrapper.cjs --noEmit",
    "clean:bun": "rm -rf dist node_modules/.cache",
    "format:bun": "biome format . --write",
    "lint:bun": "oxlint . --fix",
    "perf:test": "vitest run --reporter=verbose --isolate",
    "setup:test": "bun install && bun run build",
    "build:watch": "bun ../../scripts/tsc-wrapper.cjs --build --declaration --watch",
    "validate:types": "bun ../../scripts/tsc-wrapper.cjs --noEmit --strict",
    "generate:docs": "typedoc src/index.ts",
    "test:types": "vitest run --reporter=verbose",
    "format:types": "biome format --write src/",
    "check:duplicates": "bun run src/check-duplicate-types.ts"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "typescript": "5.9.2",
    "vitest": "^3.2.4",
    "zod": "^3.22.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "healthcare",
    "aesthetic",
    "types",
    "typescript"
  ],
  "author": "NeonPro Team",
  "license": "MIT",
  "engines": {
    "bun": ">=1.1.0"
  }
}
