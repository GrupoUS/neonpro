{
  "name": "@neonpro/types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for NeonPro aesthetic clinic platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./database": {
      "import": "./dist/database.js",
      "types": "./dist/database.d.ts"
    },
    "./healthcare": {
      "import": "./dist/healthcare.js",
      "types": "./dist/healthcare.d.ts"
    },
    "./common": {
      "import": "./dist/common.js",
      "types": "./dist/common.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "healthcare",
    "aesthetic",
    "types",
    "typescript"
  ],
  "author": "NeonPro Team",
  "license": "MIT"
}
