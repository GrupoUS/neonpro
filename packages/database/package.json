{
  "name": "@neonpro/database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:compliance": "tsx tests/test-runner.ts",
    "test:health": "tsx tests/test-runner.ts | head -20",
    "test:ci": "tsx tests/test-runner.ts && npm run test:coverage",
    "test:coverage": "bun test --coverage",
    "lint": "echo 'lint placeholder'",
    "prepare": "echo 'prepare script disabled for workspace install'",
    "db:types": "supabase gen types typescript --project-id ownkoxryswokcdanrdgj > src/types/supabase-generated.ts",
    "db:push": "prisma db push",
    "db:diff": "supabase db diff",
    "db:reset": "supabase db reset",
    "db:studio": "prisma studio",
    "sync": "npm run db:types && npm run build",
    "dev:bun": "vite dev --force",
    "build:bun": "prisma generate && tsc --noEmit",
    "build:analyze": "vite build --mode analyze",
    "test:bun": "vitest run --reporter=dot",
    "test:watch:bun": "vitest watch --reporter=dot",
    "type-check:bun": "tsc --noEmit",
    "clean:bun": "rm -rf dist node_modules/.cache",
    "format:bun": "biome format . --write",
    "lint:bun": "oxlint . --fix",
    "perf:test": "vitest run --reporter=verbose --isolate",
    "setup:test": "bun install && bun run build",
    "build:edge": "prisma generate && tsc --noEmit --outDir dist-edge",
    "test:integration:bun": "vitest run --config vitest.integration.config.ts",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "generate:client": "prisma generate",
    "db:seed": "bun run src/seed.ts",
    "validate:schema": "prisma validate",
    "format:prisma": "prisma format",
    "perf:query": "bun run src/benchmark-queries.ts"
  },
  "dependencies": {
    "@neonpro/types": "workspace:*",
    "@supabase/supabase-js": "2.58.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
