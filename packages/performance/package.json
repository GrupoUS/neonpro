{
  "name": "@neonpro/performance",
  "version": "0.1.0",
  "description": "Healthcare Performance Optimization with Intelligent Lazy Loading",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./lazy-loading": {
      "types": "./dist/lazy-loading/index.d.ts",
      "import": "./dist/lazy-loading/index.mjs",
      "require": "./dist/lazy-loading/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsup --watch",
    "lint": "oxlint",
    "format": "dprint fmt",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@neonpro/types": "workspace:*",
    "next": "^15.5.0",
    "react": "^19.1.1"
  },
  "peerDependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "@tensorflow/tfjs": "^4.22.0",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^19.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "keywords": [
    "healthcare",
    "performance",
    "lazy-loading",
    "code-splitting",
    "neonpro",
    "core-web-vitals",
    "bundle-optimization"
  ],
  "author": "NeonPro Performance Team",
  "license": "PROPRIETARY",
  "private": true
}
