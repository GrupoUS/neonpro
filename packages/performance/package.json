{
	"name": "@neonpro/performance",
	"version": "1.0.0",
	"description": "NeonPro Performance Optimization and Monitoring Package",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"clean": "rm -rf dist",
		"analyze:bundle": "node scripts/analyze-bundle.js",
		"analyze:performance": "node scripts/analyze-performance.js",
		"analyze:database": "node scripts/analyze-database.js",
		"optimize:bundles": "node scripts/optimize-bundles.js",
		"optimize:assets": "node scripts/optimize-assets.js",
		"optimize:database": "node scripts/optimize-database.js",
		"validate:performance": "node scripts/validate-performance.js",
		"validate:web-vitals": "node scripts/validate-web-vitals.js",
		"validate:mobile": "node scripts/validate-mobile.js"
	},
	"dependencies": {
		"web-vitals": "^3.5.2",
		"lighthouse": "^11.4.0",
		"webpack-bundle-analyzer": "^4.10.1",
		"@vercel/analytics": "^1.1.1",
		"@sentry/nextjs": "^7.99.0",
		"next": "^15.0.3"
	},
	"devDependencies": {
		"typescript": "^5.3.3",
		"@types/node": "^20.11.5"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"next": "^15.0.3"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./web-vitals": {
			"types": "./dist/web-vitals/index.d.ts",
			"require": "./dist/web-vitals/index.js",
			"import": "./dist/web-vitals/index.mjs"
		},
		"./bundle-analysis": {
			"types": "./dist/bundle-analysis/index.d.ts",
			"require": "./dist/bundle-analysis/index.js",
			"import": "./dist/bundle-analysis/index.mjs"
		},
		"./database": {
			"types": "./dist/database/index.d.ts",
			"require": "./dist/database/index.js",
			"import": "./dist/database/index.mjs"
		},
		"./infrastructure": {
			"types": "./dist/infrastructure/index.d.ts",
			"require": "./dist/infrastructure/index.js",
			"import": "./dist/infrastructure/index.mjs"
		}
	},
	"files": [
		"dist",
		"scripts"
	],
	"keywords": [
		"performance",
		"optimization",
		"web-vitals",
		"healthcare",
		"nextjs",
		"monitoring"
	],
	"author": "NeonPro Team",
	"license": "MIT"
}
