{
  "name": "@neonpro/ui",
  "version": "0.1.0",
  "description": "NeonPro UI Components - Simplified Healthcare Design System",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    },
    "./button": {
      "types": "./dist/src/button-only.d.ts",
      "import": "./dist/src/button-only.js",
      "require": "./dist/src/button-only.js"
    },
    "./styles": "./dist/src/styles.css",
    "./themes/neonpro": {
      "types": "./dist/src/themes/neonpro/index.d.ts",
      "import": "./dist/src/themes/neonpro/index.js",
      "require": "./dist/src/themes/neonpro/index.js"
    },
    "./themes/neonpro/components": {
      "types": "./dist/src/themes/neonpro/components/index.d.ts",
      "import": "./dist/src/themes/neonpro/components/index.js",
      "require": "./dist/src/themes/neonpro/components/index.js"
    },
    "./themes/neonpro/styles": "./dist/src/themes/neonpro/styles.css",
    "./themes/brazilian-healthcare": {
      "types": "./dist/src/themes/brazilian-healthcare/index.d.ts",
      "import": "./dist/src/themes/brazilian-healthcare/index.js",
      "require": "./dist/src/themes/brazilian-healthcare/index.js"
    },
    "./themes/brazilian-healthcare/tokens": {
      "types": "./dist/src/themes/brazilian-healthcare/tokens.d.ts",
      "import": "./dist/src/themes/brazilian-healthcare/tokens.js",
      "require": "./dist/src/themes/brazilian-healthcare/tokens.js"
    },
    "./components/dashboard/health/*": {
      "types": "./dist/src/components/dashboard/health/*.d.ts",
      "import": "./dist/src/components/dashboard/health/*.js",
      "require": "./dist/src/components/dashboard/health/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "oxlint src --fix",
    "format": "dprint fmt",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@neonpro/utils": "workspace:^",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.263.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "healthcare",
    "design-system",
    "neonpro",
    "lgpd",
    "accessibility"
  ]
}
