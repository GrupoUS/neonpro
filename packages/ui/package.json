{
  "name": "@neonpro/ui",
  "version": "0.1.0",
  "description": "NeonPro UI components library - KokonutUI and Aceternity gradient buttons",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsc -w -p tsconfig.json",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.5.0",
    "lucide-react": "^0.460.0",
    "tailwind-merge": "^2.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0 || ^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./lib/utils": {
      "types": "./src/lib/utils.ts",
      "import": "./src/lib/utils.ts",
      "require": "./src/lib/utils.ts"
    },
    "./theme": {
      "types": "./src/theme/index.ts",
      "import": "./src/theme/index.ts",
      "require": "./src/theme/index.ts"
    }
  }
}
