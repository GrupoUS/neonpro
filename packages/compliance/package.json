{
  "name": "@neonpro/compliance",
  "version": "0.1.0",
  "description": "LGPD, ANVISA, and CFM compliance automation for NeonPro healthcare platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsc --watch",
    "lint": "biome check src/",
    "lint:fix": "biome check --apply src/",
    "type-check": "npx tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "zod": "^3.22.4",
    "date-fns": "^2.30.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "@neonpro/types": "workspace:*",
    "@neonpro/config": "workspace:*",
    "@neonpro/security": "workspace:*"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted"
  }
}
