{
  "name": "@neonpro/devops",
  "version": "0.1.0",
  "private": true,
  "description": "NeonPro Healthcare DevOps Excellence - Constitutional Healthcare Testing, CI/CD, Monitoring & Infrastructure",
  "keywords": [
    "healthcare",
    "devops",
    "testing",
    "ci-cd",
    "monitoring",
    "infrastructure",
    "constitutional-healthcare",
    "medical-quality",
    "compliance-automation",
    "healthcare-security"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js",
      "require": "./dist/testing/index.js"
    },
    "./ci-cd": {
      "types": "./dist/ci-cd/index.d.ts",
      "import": "./dist/ci-cd/index.js",
      "require": "./dist/ci-cd/index.js"
    },
    "./monitoring": {
      "types": "./dist/monitoring/index.d.ts",
      "import": "./dist/monitoring/index.js",
      "require": "./dist/monitoring/index.js"
    },
    "./infrastructure": {
      "types": "./dist/infrastructure/index.d.ts",
      "import": "./dist/infrastructure/index.js",
      "require": "./dist/infrastructure/index.js"
    },
    "./security": {
      "types": "./dist/security/index.d.ts",
      "import": "./dist/security/index.js",
      "require": "./dist/security/index.js"
    },
    "./performance": {
      "types": "./dist/performance/index.d.ts",
      "import": "./dist/performance/index.js",
      "require": "./dist/performance/index.js"
    },
    "./deployment": {
      "types": "./dist/deployment/index.d.ts",
      "import": "./dist/deployment/index.js",
      "require": "./dist/deployment/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "biome lint src --write",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:healthcare": "vitest --testNamePattern=\"healthcare|medical|patient|lgpd|anvisa|cfm\"",
    "testing:validate": "node scripts/validate-testing.js",
    "ci-cd:validate": "node scripts/validate-ci-cd.js",
    "monitoring:setup": "node scripts/setup-monitoring.js",
    "infrastructure:validate": "node scripts/validate-infrastructure.js",
    "security:audit": "node scripts/security-audit.js",
    "performance:benchmark": "node scripts/performance-benchmark.js",
    "deployment:validate": "node scripts/validate-deployment.js"
  },  "dependencies": {
    "@neonpro/compliance": "workspace:*",
    "@neonpro/db": "workspace:*",
    "@neonpro/domain": "workspace:*",
    "@neonpro/types": "workspace:*",
    "@neonpro/utils": "workspace:*",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.48.2",
    "@vitest/coverage-v8": "^2.1.8",
    "vitest": "^2.1.8",
    "jsdom": "^25.0.1",
    "msw": "^2.6.6",
    "@types/supertest": "^6.0.2",
    "supertest": "^7.0.0",
    "dotenv": "^17.2.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "prometheus-api-metrics": "^3.2.2",
    "grafana-sdk": "^0.0.3",
    "docker": "^1.0.0",
    "kubernetes-client": "^12.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/node": "^22.10.2",
    "@types/winston": "^2.4.4",
    "typescript": "^5.7.2",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "next": "^15.0.0",
    "@supabase/supabase-js": "^2.55.0"
  }
}