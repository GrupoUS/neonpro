{
  "name": "@neonpro/shared",
  "version": "1.0.0",
  "description": "Shared types and schemas for NeonPro Healthcare Platform",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "type-check": "tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check src --apply"
  },
  "dependencies": {
    "@neonpro/db": "workspace:*",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.51.23",
    "hono": "^4.5.8",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.14.15",
    "@types/react": "^18.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "keywords": [
    "neonpro",
    "healthcare",
    "shared",
    "types",
    "schemas",
    "zod",
    "hono",
    "rpc"
  ],
  "author": "NeonPro Team",
  "license": "PROPRIETARY"
}