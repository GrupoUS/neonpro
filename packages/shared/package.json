{
  "name": "@neonpro/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./services/*": {
      "import": "./dist/services/*.js",
      "require": "./dist/services/*.js",
      "types": "./dist/services/*.d.ts"
    },
    "./services/structured-logging": {
      "import": "./dist/services/structured-logging.js",
      "require": "./dist/services/structured-logging.js",
      "types": "./dist/services/structured-logging.d.ts"
    },
    "./errors/*": {
      "import": "./dist/errors/*.js",
      "require": "./dist/errors/*.js",
      "types": "./dist/errors/*.d.ts"
    },
    "./errors/healthcare-errors": {
      "import": "./dist/errors/healthcare-errors.js",
      "require": "./dist/errors/healthcare-errors.js",
      "types": "./dist/errors/healthcare-errors.d.ts"
    }
  },
  "scripts": {
    "build": "bun x tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "echo 'lint placeholder'",
    "prepare": "echo 'prepare script disabled for workspace install'"
  },
  "dependencies": {
    "@neonpro/types": "workspace:*",
    "@neonpro/utils": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.57.2",
    "@opentelemetry/resources": "^1.29.0",
    "@opentelemetry/semantic-conventions": "^1.29.0",
    "@opentelemetry/exporter-jaeger": "^1.29.0",
    "@opentelemetry/sdk-trace-base": "^1.29.0",
    "@opentelemetry/sdk-metrics": "^1.29.0",
    "@opentelemetry/exporter-prometheus": "^0.57.2",
    "@opentelemetry/instrumentation-express": "^0.45.0",
    "@opentelemetry/instrumentation-http": "^0.57.2",
    "@tanstack/react-query": "^5.87.4",
    "hono": "^4.9.7",
    "nanoid": "^5.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
