{
  "name": "@neonpro/shared",
  "version": "1.0.0",
  "description": "Shared types and schemas for NeonPro Healthcare Platform",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./api-client": {
      "types": "./dist/api-client.d.ts",
      "import": "./dist/api-client.js",
      "require": "./dist/api-client.js"
    },
    "./hooks/use-healthcare-realtime": {
      "types": "./dist/hooks/use-healthcare-realtime.d.ts",
      "import": "./dist/hooks/use-healthcare-realtime.js",
      "require": "./dist/hooks/use-healthcare-realtime.js"
    },
    "./hooks/use-lgpd-realtime": {
      "types": "./dist/hooks/use-lgpd-realtime.d.ts",
      "import": "./dist/hooks/use-lgpd-realtime.js",
      "require": "./dist/hooks/use-lgpd-realtime.js"
    },
    "./hooks/use-realtime": {
      "types": "./dist/hooks/use-realtime.d.ts",
      "import": "./dist/hooks/use-realtime.js",
      "require": "./dist/hooks/use-realtime.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js",
      "require": "./dist/schemas/index.js"
    },
    "./templates": {
      "types": "./dist/templates/index.d.ts",
      "import": "./dist/templates/index.js",
      "require": "./dist/templates/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch",
    "type-check": "tsc --noEmit",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "dprint fmt",
    "format:check": "dprint check",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "@neonpro/database": "workspace:*",
    "@supabase/supabase-js": "^2.55.0",
    "hono": "^4.9.6",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "@tanstack/react-query": "^5.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.14.15",
    "@types/react": "^19.0.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "keywords": [
    "neonpro",
    "healthcare",
    "shared",
    "types",
    "schemas",
    "zod",
    "hono",
    "rpc"
  ],
  "author": "NeonPro Team",
  "license": "PROPRIETARY"
}
